{
  "name": "@rabblerouser/core-backend",
  "version": "0.1.0",
  "engines": {
    "node": "6.5.0"
  },
  "description": "A pluggable, extensible membership database for community organising",
  "main": "src/app.js",
  "scripts": {
    "ci": "npm run lint && npm test",
    "dev": "npm run prestart && nodemon index.js",
    "lint": "eslint src spec",
    "migrate": "sequelize db:migrate",
    "e2e-seed": "NODE_ENV=test node bin/e2e-seed.js",
    "pretest": "NODE_ENV=test npm run migrate && node bin/test-seed.js",
    "test": "NODE_ENV=test mocha --reporter dot --check-leaks --timeout 10000 --compilers js:babel-core/register -r spec/specGlobals --recursive spec",
    "prestart": "npm run migrate && node bin/seed.js",
    "start": "node index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/rabblerouser/core.git"
  },
  "bugs": {
    "url": "https://github.com/rabblerouser/core/issues"
  },
  "homepage": "https://github.com/rabblerouser/core#readme",
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "dependencies": {
    "@rabblerouser/stream-client": "^0.8.8",
    "bcryptjs": "^2.3.0",
    "body-parser": "^1.15.0",
    "compression": "^1.6.1",
    "connect-session-sequelize": "^3.0.0",
    "continuation-local-storage": "^3.1.7",
    "ejs": "^2.4.1",
    "express": "^4.13.4",
    "express-sanitized": "^0.5.1",
    "express-session": "^1.13.0",
    "helmet": "^1.3.0",
    "lodash": "^3.10.1",
    "moment": "^2.13.0",
    "morgan": "^1.7.0",
    "node-uuid": "^1.4.7",
    "passport": "^0.3.2",
    "passport-local": "^1.0.0",
    "pg": "^4.5.4",
    "q": "^1.4.1",
    "readline": "^1.3.0",
    "redux": "^3.6.0",
    "sequelize": "^3.22.0",
    "sequelize-cli": "^2.4.0",
    "serve-favicon": "^2.3.0",
    "underscore": "^1.8.3",
    "validator": "^5.2.0",
    "winston": "^2.2.0"
  },
  "devDependencies": {
    "aws-sdk": "^2.9.0",
    "babel-core": "^6.7.7",
    "babel-preset-es2015": "^6.6.0",
    "chai": "^3.5.0",
    "chai-as-promised": "^5.3.0",
    "eslint": "^3.14.1",
    "eslint-config-airbnb": "^14.0.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^3.0.2",
    "eslint-plugin-react": "^6.9.0",
    "mocha": "^2.4.5",
    "nodemon": "^1.11.0",
    "sinon": "^1.17.3",
    "sinon-as-promised": "^4.0.2",
    "sinon-chai": "^2.8.0",
    "supertest": "^1.2.0",
    "supertest-as-promised": "^3.1.0"
  }
}
