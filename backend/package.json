{
  "name": "rabblerouser-core-backend",
  "version": "0.1.0",
  "description": "A pluggable, extensible membership database for community organising",
  "main": "src/app.js",
  "scripts": {
    "ci": "npm run lint && npm test",
    "lint": "eslint .",
    "migrate": "sequelize --config config/db.json db:migrate",
    "pretest": "NODE_ENV=test npm run migrate",
    "test": "npm run test-jasmine && npm run test-mocha",
    "test-jasmine": "NODE_ENV=test node_modules/.bin/jasmine",
    "test-mocha": "mocha --check-leaks --timeout 10000 --compilers js:babel-core/register -r spec/support/specGlobals --recursive spec/mocha-temp/",
    "prestart": "npm run migrate",
    "start": "node ../bin/www",
    "dev:createSuperAdmin": "node ./node_scripts/createSuperAdmin.js",
    "createSuperAdmin": "NODE_ENV=production node ./node_scripts/createSuperAdmin.js"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/rabblerouser/rabblerouser-core.git"
  },
  "bugs": {
    "url": "https://github.com/rabblerouser/rabblerouser-core/issues"
  },
  "homepage": "https://github.com/rabblerouser/rabblerouser-core#readme",
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "dependencies": {
    "bcryptjs": "^2.3.0",
    "body-parser": "^1.15.0",
    "compression": "^1.6.1",
    "config": "^1.20.1",
    "connect-session-sequelize": "^3.0.0",
    "continuation-local-storage": "^3.1.7",
    "ejs": "^2.4.1",
    "express": "^4.13.4",
    "express-sanitized": "^0.5.1",
    "express-session": "^1.13.0",
    "helmet": "^1.3.0",
    "lodash": "^3.10.1",
    "moment": "^2.13.0",
    "morgan": "^1.7.0",
    "node-neat": "^1.7.2",
    "node-sass-middleware": "^0.8.0",
    "node-uuid": "^1.4.7",
    "nodemailer": "^2.3.2",
    "nodemailer-sendmail-transport": "^1.0.0",
    "passport": "^0.3.2",
    "passport-local": "^1.0.0",
    "pg": "^4.5.4",
    "q": "^1.4.1",
    "readline": "^1.3.0",
    "sequelize": "^3.22.0",
    "sequelize-cli": "^2.4.0",
    "serve-favicon": "^2.3.0",
    "underscore": "^1.8.3",
    "validator": "^5.2.0",
    "winston": "^2.2.0"
  },
  "devDependencies": {
    "babel-core": "^6.7.7",
    "babel-preset-es2015": "^6.6.0",
    "chai": "^3.5.0",
    "eslint": "^2.8.0",
    "eslint-config-airbnb": "^8.0.0",
    "eslint-plugin-import": "^1.5.0",
    "eslint-plugin-jsx-a11y": "^1.0.2",
    "eslint-plugin-react": "^5.0.1",
    "jasmine": "^2.4.1",
    "jasmine-core": "^2.4.1",
    "jasmine-reporters": "^2.1.1",
    "jasmine-sinon": "^0.4.0",
    "mocha": "^2.4.5",
    "sinon": "^1.17.3",
    "sinon-chai": "^2.8.0",
    "supertest": "^1.2.0",
    "supertest-as-promised": "^3.1.0"
  }
}
